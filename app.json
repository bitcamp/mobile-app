{
  "expo": {
    "name": "Bitcamp",
    "slug": "bitcamp",
    "sdkVersion": "36.0.0",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/imgs/app-icon.png",
    "entryPoint": "./index.jsx",
    "splash": {
      "image": "./assets/imgs/splash-screen.png",
